<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>RoutesServlet</servlet-name>
        <servlet-class>MobileTracker.RoutesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RouteServlet</servlet-name>
        <servlet-class>MobileTracker.RouteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RoutesServlet</servlet-name>
        <url-pattern>/RoutesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RouteServlet</servlet-name>
        <url-pattern>/RouteServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
